{
  "name": "xifan-nav",
  "version": "1.0.0",
  "description": "稀饭导航 - 一个基于Vue 3和TypeScript的现代化个人导航书签管理应用，支持本地存储、智能搜索、数据导入导出等功能",
  "author": "Andy Xiong",
  "license": "MIT",
  "homepage": "https://nav.xifan.uno",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ixiongdi/nav.git"
  },
  "bugs": {
    "url": "https://github.com/ixiongdi/nav/issues"
  },
  "keywords": [
    "bookmark",
    "navigation",
    "bookmark-manager",
    "vue3",
    "typescript",
    "vite",
    "element-plus",
    "indexeddb",
    "local-storage",
    "personal-tool",
    "web-app",
    "browser-bookmarks",
    "folder-management",
    "search",
    "import-export",
    "open-source"
  ],
  "private": false,
  "type": "module",
  "engines": {
    "node": ">=20.19.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "build:github": "VITE_BASE_PATH=/nav/ vue-tsc -b && vite build",
    "build:custom": "VITE_BASE_PATH=/ vue-tsc -b && vite build",
    "preview": "vite preview",
    "format": "prettier --write .",
    "lint": "vue-tsc --noEmit",
    "type-check": "vue-tsc --noEmit"
  },
  "dependencies": {
    "dexie": "^4.2.0",
    "vue": "3.5.19"
  },
  "devDependencies": {
    "@element-plus/icons-vue": "^2.3.2",
    "@types/node": "^24.3.0",
    "@types/uuid": "10.0.0",
    "@vitejs/plugin-vue": "6.0.1",
    "@vue/tsconfig": "0.8.1",
    "element-plus": "^2.10.7",
    "prettier": "^3.6.2",
    "typescript": "5.9.2",
    "uuid": "11.1.0",
    "vite": "7.1.3",
    "vue-tsc": "3.0.6"
  }
}
